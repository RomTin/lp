// if文のテスト９： if と １つの elsif と else
//                  最初の条件が偽で二つ目の条件が真のとき

int x = -10;
int y = 3;
int z = 10;
y = 2*(y+5)%9;	// y = 7
if (y > 10)  {
  // ここには来ない
  x = x*100;
  z=y+z;
}
elsif (y > 5)  {
  // ここに来る
  x = x*-100;	// x = 1000;
  z = -y+z;	// z = 3;
}
else  {
  // ここには来ない
  x = x/10;
  z = 10*z;
}
z = 5*y+z;	// z = 38


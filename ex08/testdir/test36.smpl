// if文のテスト１０： if と １つの elsif と else
//                    ２つの条件がともに偽のとき

int x = -10;
int y = 3;
int z = 10;
y = 2*(y+5)%9;	// y = 7
if (y < 2)  {
  // ここには来ない
  x = x*100;
  z=y+z;
}
elsif (y < 5)  {
  // ここには来ない
  x = x*-100;
  z = -y+z;
}
else  {
  // ここに来る
  x = x/10;	// x = -1
  z = 10*z;	// z = 100
}
z = 5*y+z;	// z = 135


   25 $@5: /* empty */

   26 stmt: REPEAT '(' expr ')' $@5 '{' stmtList '}'

   27 ifPart: IF '(' cond ')' '{' stmtList '}'
   28       | ifPart ELSIF '(' cond ')' '{' stmtList '}'

   29 elsePart: /* empty */
   30         | ELSE '{' stmtList '}'

   31 expr: expr ADDOP expr
   32     | expr MULOP expr
   33     | ADDOP expr
   34     | '(' expr ')'
   35     | vname

   36 $@6: /* empty */

   37 expr: vname '[' $@6 expr ']'
   38     | INUM
   39     | RNUM

   40 vname: ID

   41 cond: cond LOGOP cond
   42     | ULOGOP cond
   43     | expr RELOP expr
   44     | '(' cond ')'



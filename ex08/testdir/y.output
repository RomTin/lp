   18 $@2: /* empty */

   19 stmt: vname '=' $@2 expr ';'

   20 $@3: /* empty */

   21 $@4: /* empty */

   22 stmt: vname '[' $@3 expr ']' $@4 '=' expr ';'
   23     | ifPart elsePart

// 二項演算子: 引数はともに実数

real x = 8.5;		// x = 8.5
real y = 10.23;		// y = 10.23
real z = -7.81;		// z = -7.81
real w = -2.166;	// w = -2.166
real u = -12.4;		// u = -12.4 
x = x + y;		// x = 18.73
y = y - 1.85;		// y = 8.38
z = 2.5 * z;		// z = -19.525
w = w / 3.8;		// w = -0.57
u = x % u;		// u = 6.33

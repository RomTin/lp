// if文のテスト５： if と １つの elsif．最初の条件が真のとき
// 条件に || を含む

int x = -10;
int y = 3;
real z = 10.8;
y = 2*(y+5)%9;	// y = 7
if (z < 5 || x == -10.0)  {
  // ここに来る
  x = x*100;	// x = -1000
  z=y+z;	// z = 17 
}
elsif (!(z == 10) || y > 3)  {
  // ここには来ない
  x = x*-100;
  z = -y+z;
}
z = 5*y+z;	// z = 52.8

